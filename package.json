{
  "name": "space-invaders",
  "version": "0.1.0",
  "scripts": {
    "start": "npm-run-all -p watch-bsb watch-webpack",
    "watch-bsb": "bsb -make-world -w",
    "watch-webpack": "webpack -w --config webpack.config.js",
    "build": "bsb -make-world && webpack --config webpack.config.js",
    "clean": "bsb -clean-world"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "bs-platform": "^2.1.0",
    "npm-run-all": "^4.1.2",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "bs-webapi": "^0.8.2"
  }
}
